{
  "name": "testwa",
  "private": true,
  "version": "1.1.2",
  "main": "main/index.js",
  "description": "Automated test Script recording tool for Apps.",
  "author": "canfeit <canfeit@qq.com> (http://testwa.com/)",
  "license": "MIT",
  "keywords": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing"
  ],
  "tags": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing",
    "javascript",
    "ios",
    "android",
    "firefoxos"
  ],
  "homepage": "https://github.com/canfeit/testwa#readme",
  "esm": {
    "cache": false
  },
  "scripts": {
    "start": "yarn clean&&yarn start-all",
    "dir": "yarn build-umi&&npm run build -- --dir -c.asar=false",
    "build": "build",
    "release": "release-it -n&&yarn build-umi&&npm run build:all",
    "build:all": "build -w --publish always&&build -m --publish always",
    "build-cli": "npx build",
    "build-umi": "cd src/renderer && cross-env COMPRESS=none cross-env CSS_COMPRESS=none umi build",
    "start-umi": "cd src/renderer && cross-env BROWSER=none umi dev",
    "start-electron": "cd src&&nodemon --watch main --exec electron .",
    "start-all": "concurrently \"yarn start-umi\" \"wait-on http://localhost:8000 && yarn start-electron\" \"yarn start-ws\" ",
    "start-ws": "cd src/api/scrcpy && node ws.js",
    "clean": "yarn clean-bin&&yarn clean-apk",
    "clean-bin": "adb shell rm -r /data/local/tmp/mini*&&adb shell ls /data/local/tmp",
    "clean-apk": "adb uninstall io.appium.uiautomator2.server",
    "postinstall": "build install-app-deps"
  },
  "build": {
    "productName": "TestwaGen",
    "appId": "com.testwa.desktop",
    "copyright": "Copyright Â© 2018 testwa",
    "mac": {
      "target": "pkg"
    },
    "files": [
      {
        "from": "dist/renderer",
        "to": "renderer"
      },
      {
        "from": "src/main",
        "to": "main"
      },
      {
        "from": "src/api",
        "to": "api"
      },
      {
        "from": "src/common",
        "to": "common"
      },
      {
        "from": "src/public",
        "to": "renderer"
      },
      "!**/{LICENSE,LICENCE,license,*LICENSE,LICENSE*,CHANGELOG*,README*,AUTHORS,.gitmodules,.vscode,.yarnclean}",
      "!**/*.{png,jpg,JPG,PNG,jpeg,gif,map,un~}",
      "!static/",
      "!docs/",
      "!dist/",
      "!src/",
      "!tests/",
      "!deno*",
      "!yarn*",
      "!.git*",
      "!sleep.js"
    ],
    "extraResources": [
      {
        "from": "node_modules/minicap-prebuilt",
        "to": "minicap-prebuilt"
      },
      {
        "from": "node_modules/minitouch-prebuilt-beta",
        "to": "minitouch-prebuilt-beta"
      },
      "static/wappium/lib",
      "static/wappium/tests/*.txt",
      "static/wappium/*.js",
      "static/wappium/node_modules",
      "static/uiautomator2/apks"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canfeit/testwa.git"
  },
  "bugs": {
    "url": "https://github.com/canfeit/testwa/issues"
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "adbkit-apkreader": "^3.1.1",
    "await-timeout": "^1.1.1",
    "axios": "^0.19.2",
    "electron-updater": "^4.0.6",
    "express": "^4.12.3",
    "lodash": "^4.17.15",
    "minitouch-prebuilt-beta": "^1.3.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.9",
    "wd": "^1.12.1",
    "ws": "^0.7.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "ansi-to-html": "^0.6.9",
    "concurrently": "^4.1.0",
    "conventional-changelog-cli": "^2.0.12",
    "cross-env": "^5.2.0",
    "electron": "5",
    "electron-builder": "^20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "esm": "^3.2.25",
    "immutable": "^4.0.0-rc.12",
    "json-server": "^0.14.2",
    "less-vars-to-js": "^1.3.0",
    "mime-types": "^2.1.22",
    "minicap": "^0.2.1-0",
    "minicap-prebuilt": "^2.3.0",
    "nodemon": "^2.0.2",
    "pouchdb-adapter-idb": "^7.0.0",
    "react-syntax-highlighter": "^10.1.2",
    "release": "^5.0.3",
    "release-it": "^10.1.0",
    "rxdb": "^8.0.4",
    "rxjs": "^6.3.3",
    "standard-version": "^4.4.0",
    "umi": "^1.3.18",
    "umi-plugin-dva": "^0.9.1",
    "wait-on": "^3.2.0",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27",
    "yauzl": "^2.10.0"
  },
  "release-it": {
    "increment": "conventional:angular",
    "scripts": {
      "beforeStage": "npx conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "github": {
      "release": true,
      "draft": true,
      "tokenRef": "GH_TOKEN"
    },
    "npm": {
      "publish": false,
      "publishPath": "."
    },
    "git": {
      "commitArgs": "--no-verify",
      "requireCleanWorkingDir": false,
      "tagName": "v${version}"
    }
  }
}
